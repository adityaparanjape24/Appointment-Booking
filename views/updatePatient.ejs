    <div class="container col-md-5">
      <h3 class="text-center">Appointment Booking App</h3><br>
      <div class="modal-body mx-2">
        <p class="text-center mb-3">Please enter patient details </p>


        <form method="post" action="/updateRecord/<%= PatientData._id %>">
        <div class="text-center">
        <div class="form-outline form-white col-md-9 mb-4">
            <input type="FirstName" class="form-control"id="fname" name="fname" placeholder="First Name" value="<%= PatientData.First_Name %>"
                required>
        </div><br>
        <div class="form-outline form-white col-md-9 mb-4">
            <input type="LastName" class="form-control"id="lname" name="lname" placeholder="Last Name" value="<%= PatientData.Last_Name %>"
                required>
        </div><br>
        <div class="form-outline form-white col-md-9 mb-4">
            <input type="tel" class="form-control"id="Pnumber" name="Pnumber" placeholder="Mobile No" value="<%= PatientData.Contact_No %>" required>
        </div><br>
        <div class="form-outline form-white col-md-9 mb-4">
            <input type="email" class="form-control"id="P_email" name="P_email" placeholder="Email ID" value="<%= PatientData.Email_ID %>" required>
        </div><br>
        <div class="form-outline form-white col-md-9 mb-4">
            <input type="number" class="form-control"id="Age" name="Age" placeholder="Age" value="<%= PatientData.Age %>" required>
        </div><br>
        <div class="input-group mb-3 col-auto">
            <label class="input-group-text mr-3" for="inputGroupSelect01">Gender</label>
            <select type="Gender" class="form-select"id="gender" name="gender"
                required>
                <option selected>--select--</option>
                <option value="Male">Male</option>
                <option value="Female">Female</option>
            </select>
        </div>
    </div>
    <div class="modal-footer d-flex justify-content-center">
        <button type ="submit" class="btn btn-primary btn rounded-pill" role="submit"><i class="bi bi-pencil-square"></i> Update</button>
        <a href="/home" class="btn btn-success rounded-pill mr-3" role="button">Dashboard</a>
    </div>
